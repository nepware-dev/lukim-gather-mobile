{
    "presets": [
        "module:@react-native/babel-preset"
    ],
    "plugins": [
        [
            "module:react-native-dotenv"
        ],
        [
            "module-resolver",
            {
                "cwd": "babelrc",
                "root": [
                    "./src"
                ],
                "extensions": [
                    ".js",
                    ".ios.js",
                    ".android.js"
                ],
                "alias": {
                    "assets": "./src/assets",
                    "components": "./src/components",
                    "navigation": "./src/navigation",
                    "screens": "./src/screens",
                    "services": "./src/services",
                    "utils": "./src/utils",
                    "store": "./src/store",
                    "hooks": "./src/hooks",
                    "vendor": "./src/vendor",
                    "@generated": "./src/generated",
                    "@rna": "./src/vendor/react-native-arsenal/lib"
                }
            }
        ]
    ]
}
